<h1>Editar Producto</h1>
<%= form_with model: [:backoffice, @product], html: { multipart: true }, local: true do |f| %>
  <%= render partial: "form", locals: { f: f } %>
  <div class="form-actions">
    <%= f.submit "Actualizar", class: "btn-submit" %>
  </div>
<% end %>
<%= render partial: "backoffice/back_button" %>
