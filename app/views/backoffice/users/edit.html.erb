<% content_for :styles do %>
  <%= stylesheet_link_tag "backoffice/products" %>
<% end %>
<h1>Editar usuario</h1>
<%= form_with model: [:backoffice, @user], html: { multipart: true }, local: true do |f| %>
 <%= render "/devise/shared/error_messages", resource: @user %>
  <%= render partial: "edit_user_form", locals: { f: f } %> 
  <div class="form-actions">
    <%= f.submit "Actualizar", class: "btn-submit" %>
  </div>
<% end %>
<%= render partial: "backoffice/back_button" %>
